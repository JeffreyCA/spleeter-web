{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "@jeffreyca/react-jinke-music-player",
    ]
  },
  "typeRoots": [
    "src/types",
    "node_modules/@types"
  ],
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
